# Oracle Database Configuration
# 실제 운영 Oracle DB 접속 정보를 여기에 입력하세요
ORACLE_HOST=your-oracle-hostname
ORACLE_PORT=1521
ORACLE_SERVICE_NAME=your-service-name
ORACLE_USER=your-username
ORACLE_PASSWORD=your-password

# DuckDB Configuration
# DuckDB 파일 경로를 여기에 입력하세요
DUCKDB_PATH=./data/sync.duckdb
DUCKDB_LOCK_FILE=./data/sync.lock
DUCKDB_DATABASE=main

# Sync Target Table Configuration
# 동기화할 Oracle 원본 테이블명
SYNC_ORACLE_SCHEMA=your-oracle-schema-name
SYNC_ORACLE_TABLE=your-oracle-table-name
# DuckDB 대상 테이블명 (비워두면 Oracle 테이블명을 소문자로 사용)
SYNC_DUCKDB_TABLE=
# Primary Key 컬럼명
SYNC_PRIMARY_KEY=ID
# 증분 동기화용 Oracle 시간 컬럼명 (복합 인덱스인 경우 쉼표로 구분)
SYNC_TIME_COLUMN=FACTORY, TRAN_TIME
# DuckDB 쿼리용 시간 컬럼명 (설정하지 않으면 SYNC_TIME_COLUMN의 첫 번째 컬럼 사용)
# 권장: Oracle 스키마와 DuckDB 쿼리를 분리하기 위해 명시적으로 설정
DUCKDB_TIME_COLUMN=TRAN_TIME
